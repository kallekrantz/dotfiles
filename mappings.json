{
    "mappings": [
        {
            "destination": "~/.config/fish/config.fish",
            "source": "config/fish/config.fish"
        },
        {
            "destination": "~/.config/fish/fishfile",
            "source": "config/fish/fishfile"
        },
        {
            "destination": "~/.config/fish/functions/edit.fish",
            "source": "config/fish/functions/edit.fish"
        },
        {
            "destination": "~/.config/fish/functions/git.fish",
            "source": "config/fish/functions/git.fish"
        },
        {
            "destination": "~/.config/karabiner/karabiner.json",
            "source": "config/karabiner/karabiner.json"
        },
        {
            "destination": "~/.config/powerline/config.json",
            "source": "config/powerline/config.json"
        },
        {
            "destination": "~/.config/powerline/themes/shell/mytheme.json",
            "source": "config/powerline/themes/shell/mytheme.json"
        },
        {
            "destination": "~/Library/Application Support/Code/User/settings.json",
            "source": "settings.json"
        },
        {
            "destination": "~/.slate",
            "source": "slate"
        },
        {
            "destination": "~/.slate.js",
            "source": "slate.js"
        },
        {
            "destination": "~/.spacemacs",
            "source": "spacemacs"
        },
        {
            "destination": "~/.vimrc",
            "source": "vimrc"
        },
        {
            "destination": "~/.zshenv",
            "source": "zshenv"
        },
        {
            "destination": "~/.zshrc",
            "source": "zshrc"
        },
        {
            "destination": "~/.xmonad.hs",
            "source": "xmonad.hs"
        }
    ]
}
